{
  "name": "react-native-module-paths",
  "version": "1.0.0",
  "description": "A small dev server to automatically sync your module paths for RAM bundling",
  "license": "ISC",
  "author": "Alex Figliolia",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "rn-module-paths": "dist/cjs/CLI.js"
  },
  "files": [
    "dist",
    "src/*"
  ],
  "scripts": {
    "build": "npx ts-packager -e src",
    "cli": "npx ts-node src/CLI.ts",
    "lint": "ts-node ci/commands/Lint.ts",
    "test": "ts-node ci/commands/Test.ts"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "chalk": "4.1.2"
  },
  "devDependencies": {
    "@figliolia/child-process": "^1.0.3",
    "@figliolia/task-queue": "^1.0.2",
    "@figliolia/ts-packager": "^1.0.6",
    "@swc/core": "^1.3.99",
    "@types/chalk": "^2.2.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.11.5",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.4.2"
  }
}